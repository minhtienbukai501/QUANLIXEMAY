	public Vector<Account> layDuLieuDangNhap()
	{
		Vector<Account> vec = new  Vector<Account>()
		try {
			String sql = "select * from acccount";
			Statement statement = conn.createStatement();
			ResultSet result = statement.executeQuery(sql);
			
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		return vec;
	}
	
	
}
